#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/qinwei/Desktop/Project/青训营项目/YOUTH-UI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/qinwei/Desktop/Project/青训营项目/YOUTH-UI/node_modules/.pnpm/node_modules"
fi
"$basedir/../../../../node_modules/.pnpm/esbuild@0.15.18/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
